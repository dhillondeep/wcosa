{
  "app": {
    "paths": [
      {
        "constraints": [],
        "entry": "/",
        "files": [
          {
            "constraints": ["extra"],
            "from": "templates/gitignore/.gitignore-general",
            "to": ".gitignore",
            "override": false,
            "update": false
          },
          {
            "constraints": ["extra"],
            "from": "templates/readme/APP_README.md",
            "to": "README.md",
            "override": false,
            "update": false
          },
          {
            "constraints": ["ide=clion"],
            "from": "templates/ide/clion/app/CMakeLists.txt.tpl",
            "to": "CMakeLists.txt",
            "override": false,
            "update": true
          },
          {
            "constraints": ["ide=clion"],
            "from": "templates/ide/clion/app/CMakeListsPrivate.txt.tpl",
            "to": "CMakeListsPrivate.txt",
            "override": true,
            "update": true
          }
        ]
      },
      {
        "constraints": [],
        "entry": "/src",
        "files": [
          {
            "constraints": ["example"],
            "from": "templates/sample-program/app/main.cpp",
            "to": "main.cpp",
            "override": false,
            "update": false
          }
        ]
      }
    ]
  },
  "pkg": {
    "paths": [
      {
        "constraints": [],
        "entry": "/",
        "files": [
          {
            "constraints": ["extra"],
            "from": "templates/gitignore/.gitignore-general",
            "to": ".gitignore",
            "override": false,
            "update": false
          },
          {
            "constraints": ["extra"],
            "from": "templates/readme/PKG_README.md",
            "to": "README.md",
            "override": false,
            "update": false
          },
          {
            "constraints": ["ide=clion"],
            "from": "templates/ide/clion/app/CMakeLists.txt.tpl",
            "to": "CMakeLists.txt",
            "override": false,
            "update": true
          },
          {
            "constraints": ["ide=clion"],
            "from": "templates/ide/clion/app/CMakeListsPrivate.txt.tpl",
            "to": "CMakeListsPrivate.txt",
            "override": true,
            "update": true
          }
        ]
      },
      {
        "constraints": ["no-header-only"],
        "entry": "/src",
        "files": [
          {
            "constraints": ["example"],
            "from": "templates/sample-program/pkg/output.cpp",
            "to": "output.cpp",
            "override": false,
            "update": false
          }
        ]
      },
      {
        "constraints": [],
        "entry": "/include",
        "files": [
          {
            "constraints": ["example", "no-header-only"],
            "from": "templates/sample-program/pkg/output.h",
            "to": "output.h",
            "override": false,
            "update": false
          },
          {
            "constraints": ["example", "header-only"],
            "from": "templates/sample-program/pkg-header-only/printer.h",
            "to": "printer.h",
            "override": false,
            "update": false
          }
        ]
      },
      {
        "constraints": [],
        "entry": "/tests",
        "files": [
          {
            "constraints": ["example", "no-header-only"],
            "from": "templates/sample-program/pkg/main.cpp",
            "to": "main.cpp",
            "override": false,
            "update": false
          },
          {
            "constraints": ["example", "header-only"],
            "from": "templates/sample-program/pkg-header-only/main.cpp",
            "to": "main.cpp",
            "override": false,
            "update": false
          }
        ]
      }
    ]
  }
}
